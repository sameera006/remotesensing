  author = {Xavier, Alexandre Cândido and Rudorff, Bernardo F. T. and Shimabukuro, Yosio Edemir and Berka, Luciana Miura Sugawara and Moreira, Mauricio Alves},
  date = {2006-02-20},
  journaltitle = {International Journal of Remote Sensing},
  volume = {27},
