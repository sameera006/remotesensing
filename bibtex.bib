@article{fan2020,
  title = {Explicitly {{Identifying}} the {{Desertification Change}} in {{CMREC Area Based}} on {{Multisource Remote Data}}},
  author = {Fan, Zemeng and Li, Saibo and Fang, Haiyan},
  date = {2020-01},
  journaltitle = {Remote Sensing},
  volume = {12},
