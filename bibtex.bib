@article{merry2014,
  title = {Urban Tree Cover Change in {{Detroit}} and {{Atlanta}}, {{USA}}, 1951–2010},
  author = {Merry, Krista and Siry, Jacek and Bettinger, Pete and Bowker, J. M.},
